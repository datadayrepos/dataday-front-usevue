{
  "name": "@datadayrepos/usevuecore",
  "version": "10.5.0-3",
  "description": "Collection of essential Vue Composition Utilities: Light version of original",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/datadayrepos/front-usevue.git",
    "directory": "packages/core"
  },
  "keywords": [
    "vue",
    "vue-use",
    "utils"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.cjs"
    },
    "./*": "./*",
    "./metadata": {
      "import": "./metadata.mjs",
      "require": "./metadata.cjs"
    }
  },
  "main": "./index.cjs",
  "module": "./index.mjs",
  "unpkg": "./index.iife.min.js",
  "jsdelivr": "./index.iife.min.js",
  "types": "./index.d.cts",
  "dependencies": {
    "@datadayrepos/usevueshared": "workspace:*"
  }
}
